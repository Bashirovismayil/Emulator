<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Task4_2">


    <ImageView
        android:id="@+id/rectangles3"
        android:layout_width="148dp"
        android:layout_height="183dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="56dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/soft_rect">

    </ImageView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="124dp"
        android:text="Support"
        app:layout_constraintEnd_toEndOf="@id/rectangles3"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="@+id/rectangles3"
        app:layout_constraintTop_toTopOf="@id/rectangles3">

    </TextView>

    <ImageView
        android:id="@+id/ellipse3"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="84dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ellipse_icon">

    </ImageView>

    <ImageView
        android:id="@+id/support"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="100dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/technical_support">

    </ImageView>

    <ImageView
        android:id="@+id/rectangles4"
        android:layout_width="148dp"
        android:layout_height="183dp"
        android:layout_marginStart="212dp"
        android:layout_marginTop="56dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/soft_rect">

    </ImageView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="124dp"
        android:text="Geo Support"
        app:layout_constraintEnd_toEndOf="@id/rectangles4"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="@+id/rectangles4"
        app:layout_constraintTop_toTopOf="@id/rectangles4">

    </TextView>

    <ImageView
        android:id="@+id/ellipse4"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="248dp"
        android:layout_marginTop="84dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ellipse_icon">

    </ImageView>

    <ImageView
        android:id="@+id/geosupport"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/ellipse4"
        app:layout_constraintEnd_toEndOf="@+id/ellipse4"
        app:layout_constraintHorizontal_bias="1.6"
        app:layout_constraintStart_toStartOf="@+id/ellipse4"
        app:layout_constraintTop_toTopOf="@+id/ellipse4"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/geo_icon">

    </ImageView>


</androidx.constraintlayout.widget.ConstraintLayout>